android.x86_64.single.debug = "./bin/libOptiTrack-plugin.android.template_debug.x86_64.so"
android.x86_64.double.debug = "./bin/libOptiTrack-plugin.android.template_debug.x86_64.double.so"
android.x86_64.single.release = "./bin/libOptiTrack-plugin.android.template_release.x86_64.so"
