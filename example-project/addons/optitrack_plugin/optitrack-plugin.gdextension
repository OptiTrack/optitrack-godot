[configuration]

entry_symbol = "motive_client_init"
compatibility_minimum = "4.1"

; reload the extension upon recompilation
reloadable = true

[libraries]
; Relative paths ensure that our GDExtension can be placed anywhere in the project directory.
macos.single.debug = "./bin/libOptiTrack-plugin.macos.template_debug.dylib"
macos.double.debug = "./bin/libOptiTrack-plugin.macos.template_debug.double.dylib"
macos.single.release = "./bin/libOptiTrack-plugin.macos.template_release.dylib"
macos.double.release = "./bin/libOptiTrack-plugin.macos.template_release.double.dylib"

ios.arm64.single.debug = "./bin/libOptiTrack-plugin.ios.template_debug.arm64.dylib"
ios.arm64.double.debug = "./bin/libOptiTrack-plugin.ios.template_debug.arm64.double.dylib"
ios.arm64.single.release = "./bin/libOptiTrack-plugin.ios.template_release.arm64.dylib"
ios.arm64.double.release = "./bin/libOptiTrack-plugin.ios.template_release.arm64.double.dylib"

windows.x86_32.single.debug = "./bin/OptiTrack-plugin.windows.template_debug.x86_32.dll"
windows.x86_32.double.debug = "./bin/OptiTrack-plugin.windows.template_debug.x86_32.double.dll"
windows.x86_32.single.release = "./bin/OptiTrack-plugin.windows.template_release.x86_32.dll"
windows.x86_32.double.release = "./bin/OptiTrack-plugin.windows.template_release.x86_32.double.dll"

windows.x86_64.single.debug = "./bin/OptiTrack-plugin.windows.template_debug.x86_64.dll"
