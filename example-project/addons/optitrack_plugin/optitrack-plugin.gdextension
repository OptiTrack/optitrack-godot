windows.x86_64.double.release = "./bin/OptiTrack-plugin.windows.template_release.x86_64.double.dll"

linux.x86_64.single.debug = "./bin/libOptiTrack-plugin.linux.template_debug.x86_64.so"
linux.x86_64.double.debug = "./bin/libOptiTrack-plugin.linux.template_debug.x86_64.double.so"
linux.x86_64.single.release = "./bin/libOptiTrack-plugin.linux.template_release.x86_64.so"
linux.x86_64.double.release = "./bin/libOptiTrack-plugin.linux.template_release.x86_64.double.so"

linux.arm64.single.debug = "./bin/libOptiTrack-plugin.linux.template_debug.arm64.so"
linux.arm64.double.debug = "./bin/libOptiTrack-plugin.linux.template_debug.arm64.double.so"
